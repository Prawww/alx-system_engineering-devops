#!/usr/bin/env bash
#include<stdio.h>

int main(int argc, char *argv[])
{
	int count = argc;

	int c = 0;
	while (argv[c] == "start")
	{
		/etc/init.d/manage_my_process start;
		printf("manage_my_process started");
		c++;
	}

	while (argv[c] == "stop")
	{
		/etc/init.d/manage_my_process sto;
		printf("manage_my_process stopped");
		c++;
	}
	return 0;
}
